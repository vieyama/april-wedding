// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./file.db"
}

model guest {
  id         String  @id @default(cuid())
  name       String
  message    String
  countGuest Int?
  join       Boolean? @default(false)
}
